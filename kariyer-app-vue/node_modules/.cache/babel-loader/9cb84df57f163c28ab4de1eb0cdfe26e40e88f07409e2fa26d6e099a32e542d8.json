onents:{\n      Navbar\n    },\n    data(){\n      return{\n        showNavbar:false,\n        user:null,\n        \n      }\n    },\n    created() {\n      \n      const activeUserId = localStorage.getItem(\"ActiveUserId\");\n      const activeUserType = localStorage.getItem(\"ActiveUserType\");\n      \n      if (activeUserId && activeUserId !== '-1') {\n        this.user = {\n          id: activeUserId,\n          type: activeUserType\n        };\n        this.showNavbar = true;\n      }\n    },\n    \n    \n\n    methods: {\n    handleLoginSuccess(user) {\n      this.user = user;\n      localStorage.setItem(\"ActiveUserId\",user.id);\n      localStorage.setItem(\"ActiveUserType\",user.type);\n      \n      this.showNavbar = true;\n      this.$router.push('/home');\n    },\n    hanldeExit(){\n      localStorage.setItem(\"ActiveUserId\",-1);\n      localStorage.setItem(\"ActiveUserType\",\"\");\n      this.showNavbar=false;\n    },\n  }\n    \n    \n\n}\n    \n    \n</script>\n\n<style scoped>\n.app-container {\n  display: flex;\n  flex-direction: column;\n  height: 100vh;\n}\n\n.content-area {\n  position: relative;\n  flex: 1;\n  overflow: hidden;\n}\n\n.background-image {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-image: url('@/../public/stairs.jpg');\n  background-size: cover;\n  background-position: center;\n  background-repeat: no-repeat;\n  z-index: -1; /* Arka planı içerikten arka plana koyar */\n}\n\n.router-view-container {\n  position: relative;\n  background-color: white; /* Beyaz arka plan */\n  height: 100%;\n  overflow-y: auto; /* Kaydırma desteği */\n  padding: 20px; /* İçerik alanı için boşluk */\n}\n</style>"],"mappings":";AAwBA,OAAOA,MAAK,MAAO,yBAAyB;AAO5C,eAAc;EACVC,UAAU,EAAC;IACTD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,UAAU,EAAC,KAAK;MAChBC,IAAI,EAAC;IAEP;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IAER,MAAMC,YAAW,GAAIC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;IACzD,MAAMC,cAAa,GAAIF,YAAY,CAACC,OAAO,CAAC,gBAAgB