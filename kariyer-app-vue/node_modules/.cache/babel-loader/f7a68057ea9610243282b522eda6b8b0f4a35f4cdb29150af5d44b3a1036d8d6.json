form-check-label\" for=\"unpublishedCheck\">Yayınlanmamış</label>\r\n      </div>\r\n    </div>\r\n    \r\n    <table class=\"table table-hover\">\r\n      <thead>\r\n        <tr>\r\n          <th scope=\"col\" @click=\"sortTable('index')\">\r\n            #\r\n            <span v-if=\"sortKey === 'index'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('jobTitle')\">\r\n            İş Başlığı \r\n            <span v-if=\"sortKey === 'jobTitle'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('locations')\">\r\n            Konum\r\n            <span v-if=\"sortKey === 'locations'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('experience')\">\r\n            Deneyim\r\n            <span v-if=\"sortKey === 'experience'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('isPublished')\">\r\n            Yayınlanma Durumu\r\n            <span v-if=\"sortKey === 'isPublished'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\">İşlemler</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(advert, index) in sortedAdverts\" :key=\"index\">\r\n          <td>{{ index + 1 }}</td>\r\n          <td>{{ advert.jobTitle }}</td>\r\n          <td>{{ advert.locations.join(\", \") }}</td>\r\n          <td>{{ advert.experience }} Yıl</td>\r\n          <td>{{ advert.isPublished ? \"Yayınlandı\" : \"Yayınlanmadı\" }}</td>\r\n          <td>\r\n            <button class=\"btn btn-primary\" @click=\"editAdvert(index)\">Değiştir</button>\r\n            <button class=\"btn btn-danger\" @click=\"deleteAdvert(index)\">Sil</button>\r\n            <button type=\"button\" class=\"btn btn-warning\" @click=\"togglePublication(index)\">\r\n              {{ advert.isPu