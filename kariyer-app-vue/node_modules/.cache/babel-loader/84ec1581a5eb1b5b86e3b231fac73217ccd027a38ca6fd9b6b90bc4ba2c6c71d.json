{"ast":null,"code":"import Navbar from '../Navbar.vue';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      users: [],\n      sortKey: '',\n      sortOrder: 'asc'\n    };\n  },\n  computed: {\n    sortedUsers() {\n      return this.users.sort((a, b) => {\n        let modifier = this.sortOrder === 'asc' ? 1 : -1;\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\n        return 0;\n      });\n    }\n  },\n  methods: {\n    sortTable(key) {\n      this.sortKey = key;\n      this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\n    },\n    editUser(id) {\n      // Kullanıcıyı düzenleme işlemi burada gerçekleştirilecek\n      console.log('Edit user with id:', id);\n    },\n    deleteUser(id) {\n      // Kullanıcıyı silme işlemi burada gerçekleştirilecek\n      console.log('Delete user with id:', id);\n    }\n  },\n  created() {\n    axios.get(`https://localhost:7190/get/GetUsers`).then(res => {\n      this.users = res.data;\n      console.log(this.users);\n    }).catch(err => {\n      console.error('Error fetching users:', err);\n    });\n  }\n};","map":{"version":3,"names":["Navbar","axios","data","users","sortKey","sortOrder","computed","sortedUsers","sort","a","b","modifier","methods","sortTable","key","editUser","id","console","log","deleteUser","created","get","then","res","catch","err","error"],"sources":["C:\\Users\\Furkan\\Desktop\\Niso proje Kariyer-App\\kariyer-app-vue\\src\\components\\AdminAndComponents\\UserList.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <Navbar></Navbar>\r\n      <table class=\"table table-hover\">\r\n        <thead>\r\n          <tr>\r\n            <th scope=\"col\" @click=\"sortTable('id')\">\r\n              Kullanıcı ID\r\n              <span v-if=\"sortKey === 'id'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n            </th>\r\n            <th scope=\"col\" @click=\"sortTable('email')\">\r\n              E-posta\r\n              <span v-if=\"sortKey === 'email'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n            </th>\r\n            <th scope=\"col\" @click=\"sortTable('type')\">\r\n              Kullanıcı Türü\r\n              <span v-if=\"sortKey === 'type'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n            </th>\r\n            <th scope=\"col\">İşlemler</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"user in sortedUsers\" :key=\"user.id\">\r\n            <td>{{ user.id || \" \" }}</td>\r\n            <td>{{ user.email || \" \" }}</td>\r\n            <td>{{ user.type || \" \" }}</td>\r\n            <td>\r\n              <button class=\"btn btn-primary\" @click=\"editUser(user.id)\">Düzenle</button>\r\n              <button class=\"btn btn-danger\" @click=\"deleteUser(user.id)\">Sil</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import Navbar from '../Navbar.vue';\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        users: [],\r\n        sortKey: '',\r\n        sortOrder: 'asc'\r\n      };\r\n    },\r\n    computed: {\r\n      sortedUsers() {\r\n        return this.users.sort((a, b) => {\r\n          let modifier = this.sortOrder === 'asc' ? 1 : -1;\r\n          if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\r\n          if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\r\n          return 0;\r\n        });\r\n      }\r\n    },\r\n    methods: {\r\n      sortTable(key) {\r\n        this.sortKey = key;\r\n        this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\r\n      },\r\n      editUser(id) {\r\n        // Kullanıcıyı düzenleme işlemi burada gerçekleştirilecek\r\n        console.log('Edit user with id:', id);\r\n      },\r\n      deleteUser(id) {\r\n        // Kullanıcıyı silme işlemi burada gerçekleştirilecek\r\n        console.log('Delete user with id:', id);\r\n      }\r\n    },\r\n    created() {\r\n      axios\r\n        .get(`https://localhost:7190/get/GetUsers`)\r\n        .then(res => {\r\n          this.users = res.data;\r\n          console.log(this.users);\r\n        })\r\n        .catch(err => {\r\n          console.error('Error fetching users:', err);\r\n        });\r\n    }\r\n  };\r\n  </script>\r\n    "],"mappings":"AAqCE,OAAOA,MAAK,MAAO,eAAe;AAClC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACJ,KAAK,CAACK,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QAC/B,IAAIC,QAAO,GAAI,IAAI,CAACN,SAAQ,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QAChD,IAAII,CAAC,CAAC,IAAI,CAACL,OAAO,IAAIM,CAAC,CAAC,IAAI,CAACN,OAAO,CAAC,EAAE,OAAO,CAAC,IAAIO,QAAQ;QAC3D,IAAIF,CAAC,CAAC,IAAI,CAACL,OAAO,IAAIM,CAAC,CAAC,IAAI,CAACN,OAAO,CAAC,EAAE,OAAO,IAAIO,QAAQ;QAC1D,OAAO,CAAC;MACV,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,GAAG,EAAE;MACb,IAAI,CAACV,OAAM,GAAIU,GAAG;MAClB,IAAI,CAACT,SAAQ,GAAI,IAAI,CAACA,SAAQ,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;IAC5D,CAAC;IACDU,QAAQA,CAACC,EAAE,EAAE;MACX;MACAC,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEF,EAAE,CAAC;IACvC,CAAC;IACDG,UAAUA,CAACH,EAAE,EAAE;MACb;MACAC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,EAAE,CAAC;IACzC;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACRnB,KAAI,CACDoB,GAAG,CAAC,qCAAqC,EACzCC,IAAI,CAACC,GAAE,IAAK;MACX,IAAI,CAACpB,KAAI,GAAIoB,GAAG,CAACrB,IAAI;MACrBe,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,KAAK,CAAC;IACzB,CAAC,EACAqB,KAAK,CAACC,GAAE,IAAK;MACZR,OAAO,CAACS,KAAK,CAAC,uBAAuB,EAAED,GAAG,CAAC;IAC7C,CAAC,CAAC;EACN;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}