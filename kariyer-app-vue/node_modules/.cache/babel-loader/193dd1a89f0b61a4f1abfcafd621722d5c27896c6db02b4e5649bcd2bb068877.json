: -1;\r\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\r\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\r\n        return 0;\r\n      });\r\n    }\r\n  },\r\n  created() {\r\n    this.adverts = JSON.parse(localStorage.getItem('adverts')) || [];\r\n  },\r\n  methods: {\r\n    sortTable(key) {\r\n      if (this.sortKey === key) {\r\n        this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\r\n      } else {\r\n        this.sortKey = key;\r\n        this.sortOrder = 'asc';\r\n      }\r\n    },\r\n    deleteAdvert(index) {\r\n      this.adverts.splice(index, 1);\r\n      localStorage.setItem('adverts', JSON.stringify(this.adverts));\r\n    },\r\n    editAdvert(index) {\r\n      this.$router.push({ path: `/edit/${index}` });\r\n    },\r\n    togglePublication(index) {\r\n      this.adverts[index].isPublished = !this.adverts[index].isPublished;\r\n      localStorage.setItem('adverts', JSON.stringify(this.adverts));\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";AA4CA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MAAS;MACpBC,SAAS,EAAE,KAAK,CAAI;IACtB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACJ,OAAO,CAACK,KAAK,CAAC,CAAC,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACzC,IAAIC,QAAO,GAAI,IAAI,CAACP,SAAQ,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QAChD,IAAIK,CAAC,CAAC,IAAI,CAACN,OAAO,IAAIO,CAAC,CAAC,IAAI,CAACP,OAAO,CAAC,EAAE,OAAO,CAAC,IAAIQ,QAAQ;QAC3D,IAAIF,CAAC,CAAC,IAAI,CAACN,OAAO,IAAIO,CAAC,CAAC,IAAI,CAACP,OAAO,CAAC,EAAE,OAAO,IAAIQ,QAAQ;QAC1D,OAAO,CAAC;MACV,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACV,OAAM,GAAIW,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,KAAK,EAAE;EAClE,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,GAAG,EAAE;MACb,IAAI,IAAI,CAAChB,OAAM,KAAMgB,GAAG,EAAE;QACxB,IAAI,CAACf,SAAQ,GAAI,IAAI,CAACA,SAAQ,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;MAC5D,OAAO;QACL,IAAI,CAACD,OAAM,GAAIgB,GAAG;QAClB,IAAI,CAACf,SAAQ,GAAI,KAAK;MACxB;IACF,CAAC;IACDgB,YAAYA,CA