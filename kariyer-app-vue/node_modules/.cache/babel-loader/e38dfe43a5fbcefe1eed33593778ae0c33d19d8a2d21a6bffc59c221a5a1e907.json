{"ast":null,"code":"import Navbar from '../Navbar.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      searchQuery: '',\n      cvs: []\n    };\n  },\n  created() {\n    axios.get(`http://localhost:7190/cv`).then(res => {\n      this.cvs = res.data;\n      console.log(this.cvs);\n    }).catch(err => {\n      console.error('Error fetching adverts:', err);\n    });\n  },\n  methods: {\n    deleteCv(id) {\n      axios.delete(`http://localhost:7190/cv/${id}`);\n      location.reload();\n    }\n  },\n  computed: {\n    filteredCvs() {\n      return this.cvs.filter(cv => {\n        const searchLower = this.searchQuery.toLowerCase();\n        return cv.id.toString().includes(searchLower) || cv.contactInfo.email.toLowerCase().includes(searchLower) || cv.personalInfo.firstName.toLowerCase().includes(searchLower) || cv.personalInfo.lastName.toLowerCase().includes(searchLower);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Navbar","axios","components","data","searchQuery","cvs","created","get","then","res","console","log","catch","err","error","methods","deleteCv","id","delete","location","reload","computed","filteredCvs","filter","cv","searchLower","toLowerCase","toString","includes","contactInfo","email","personalInfo","firstName","lastName"],"sources":["C:\\Users\\Furkan\\Desktop\\Niso proje Kariyer-App\\kariyer-app-vue\\src\\components\\AdminAndComponents\\CvList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\" v-if=\"cvs\">\r\n    <input \r\n      type=\"text\" \r\n      v-model=\"searchQuery\" \r\n      placeholder=\"Ara...\" \r\n      class=\"form-control mb-3\"\r\n    />\r\n\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>ID</th>\r\n          <th>E-mail</th>\r\n          <th>İsim isim</th>\r\n          <th>İşlemler</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr \r\n          v-for=\"cv in filteredCvs\" \r\n          :key=\"cv.id\"\r\n        >\r\n          <td>{{ cv.id }}</td>\r\n          <td>{{ cv.contactInfo.email }}</td>\r\n          <td>{{ cv.personalInfo.firstName }} {{ cv.personalInfo.lastName }}</td>\r\n          <td><button class=\"btn btn-danger\" @click=\"deleteCv (cv.id)\">Sil</button></td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>  \r\n</template>\r\n\r\n  \r\n\r\n<script>\r\nimport Navbar from '../Navbar.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  components:{\r\n    Navbar\r\n  },\r\n    data(){\r\n        return{\r\n            searchQuery: '',\r\n            cvs:[],\r\n        }\r\n    },\r\n    created(){\r\n        axios.get(`http://localhost:7190/cv`)\r\n        .then(res => {\r\n            this.cvs = res.data;\r\n            console.log(this.cvs);\r\n        })\r\n        .catch(err => {\r\n            console.error('Error fetching adverts:', err);\r\n        });\r\n    },\r\n    methods:{\r\n      deleteCv(id){\r\n        axios.delete(`http://localhost:7190/cv/${id}`)\r\n        location.reload();\r\n      }\r\n    },\r\n    computed: {\r\n    filteredCvs() {\r\n      return this.cvs.filter(cv => {\r\n        const searchLower = this.searchQuery.toLowerCase();\r\n        return (\r\n          cv.id.toString().includes(searchLower) ||\r\n          cv.contactInfo.email.toLowerCase().includes(searchLower) ||\r\n          cv.personalInfo.firstName.toLowerCase().includes(searchLower)||\r\n          cv.personalInfo.lastName.toLowerCase().includes(searchLower)\r\n\r\n        );\r\n      });\r\n    },\r\n  },\r\n}\r\n\r\n</script>"],"mappings":"AAoCA,OAAOA,MAAK,MAAO,eAAe;AAClC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAC;IACTF;EACF,CAAC;EACCG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,WAAW,EAAE,EAAE;MACfC,GAAG,EAAC;IACR;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACLL,KAAK,CAACM,GAAG,CAAC,0BAA0B,EACnCC,IAAI,CAACC,GAAE,IAAK;MACT,IAAI,CAACJ,GAAE,GAAII,GAAG,CAACN,IAAI;MACnBO,OAAO,CAACC,GAAG,CAAC,IAAI,CAACN,GAAG,CAAC;IACzB,CAAC,EACAO,KAAK,CAA