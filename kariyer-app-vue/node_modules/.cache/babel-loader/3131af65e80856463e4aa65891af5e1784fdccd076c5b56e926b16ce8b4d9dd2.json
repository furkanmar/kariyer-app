{"ast":null,"code":"import NavbarLink from './NavbarLink.vue';\nexport default {\n  components: {\n    NavbarLink\n  },\n  props: ['pages', 'activePage', 'navLinkClick'],\n  data() {\n    return {\n      theme: 'dark',\n      backgroundColor: 'white',\n      themeWord: 'Aydınlık Mod',\n      wordColor: 'black'\n    };\n  },\n  methods: {\n    changeTheme() {\n      let theme = 'light';\n      let backgroundColor = 'black';\n      let themeWord = 'Gece Modu';\n      let wordColor = 'white';\n      if (this.theme == 'light') {\n        theme = 'dark';\n        backgroundColor = 'white';\n        themeWord = 'Aydınlık Mod';\n        wordColor = 'black';\n      }\n      this.theme = theme;\n      this.backgroundColor = backgroundColor;\n      this.themeWord = themeWord;\n      this.wordColor = wordColor;\n      this.storeThemeSetting();\n    },\n    storeThemeSetting() {\n      localStorage.setItem('theme', this.theme);\n      localStorage.setItem('backgroundColor', this.backgroundColor);\n      localStorage.setItem('themeWord', this.themeWord);\n      localStorage.setItem('wordColor', this.wordColor);\n    },\n    getThemeSetting() {\n      let theme = localStorage.getItem('theme');\n      let backgroundColor = localStorage.getItem('backgroundColor');\n      let themeWord = localStorage.getItem('themeWord');\n      let wordColor = localStorage.getItem('wordColor');\n      if (theme | backgroundColor | themeWord | wordColor) {\n        this.theme = theme;\n        this.backgroundColor = backgroundColor;\n        this.themeWord = themeWord;\n        this.wordColor = wordColor;\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarLink","components","props","data","theme","backgroundColor","themeWord","wordColor","methods","changeTheme","storeThemeSetting","localStorage","setItem","getThemeSetting","getItem"],"sources":["C:\\Users\\Furkan\\Documents\\Pratik kod\\NisoPratik\\kariyer-app\\src\\components\\Navbar.vue"],"sourcesContent":["<template>\r\n<nav \r\n        \r\n        :class=\"[`navbar-${theme}`,`bg-${theme}`,'navbar','navbar-expand-lg']\"\r\n        >\r\n            <div class=\"container-fluid\">\r\n                <a href=\"#\" class=\"navbar-brand\">Kariyer App</a>\r\n                <ul class=\"navbar-nav me-auto mb-2 mb-lg-0\">\r\n                    <li v-for=\"(page, index) in pages\" key=\"index\" class=\"nav-item\" :key=\"index\">\r\n                        <navbar-link \r\n                        :page=\"page\"\r\n                        :isActive=\"activePage==index\"\r\n                        @click.prevent=\"navLinkClick(index)\"\r\n                        >\r\n                        </navbar-link>\r\n                        \r\n                    </li>\r\n                    \r\n                    \r\n                </ul>\r\n                <form \r\n                class=\"d-flex\">\r\n                <button\r\n                class=\"btn btn-primary\"\r\n                :style=\"{'background-color':backgroundColor , 'color':wordColor}\"\r\n                @click.prevent=changeTheme()>{{themeWord}}</button>\r\n                </form>\r\n                \r\n            </div>\r\n        </nav>\r\n\r\n</template>\r\n\r\n<script>\r\nimport NavbarLink from './NavbarLink.vue';\r\n\r\nexport default{\r\n    components:{\r\n        NavbarLink,\r\n\r\n    },\r\n    props:['pages','activePage','navLinkClick'],\r\n    data(){\r\n        return{\r\n            theme: 'dark',\r\n            backgroundColor: 'white',\r\n            themeWord:'Aydınlık Mod',\r\n            wordColor:'black',\r\n        }\r\n    },\r\n\r\n    methods:{\r\n        changeTheme(){\r\n            let theme='light';\r\n            let backgroundColor='black';\r\n            let themeWord='Gece Modu';\r\n            let wordColor='white'\r\n            if(this.theme=='light') {\r\n                theme='dark';\r\n                backgroundColor='white';\r\n                themeWord='Aydınlık Mod';\r\n                wordColor='black'\r\n            }\r\n            this.theme=theme;\r\n            this.backgroundColor=backgroundColor;\r\n            this.themeWord=themeWord;\r\n            this.wordColor=wordColor;\r\n            this.storeThemeSetting();\r\n            \r\n            \r\n        },\r\n        storeThemeSetting(){\r\n            localStorage.setItem('theme',this.theme);\r\n            localStorage.setItem('backgroundColor',this.backgroundColor);\r\n            localStorage.setItem('themeWord',this.themeWord);\r\n            localStorage.setItem('wordColor',this.wordColor);\r\n        },\r\n        getThemeSetting(){\r\n            let theme=localStorage.getItem('theme');\r\n            let backgroundColor=localStorage.getItem('backgroundColor');\r\n            let themeWord=localStorage.getItem('themeWord');\r\n            let wordColor=localStorage.getItem('wordColor');\r\n            if(theme|backgroundColor|themeWord|wordColor){\r\n                this.theme=theme;\r\n                this.backgroundColor=backgroundColor;\r\n                this.themeWord=themeWord;\r\n                this.wordColor=wordColor;\r\n\r\n            }\r\n        },\r\n    }\r\n}\r\n\r\n        \r\n</script>"],"mappings":"AAkCA,OAAOA,UAAS,MAAO,kBAAkB;AAEzC,eAAc;EACVC,UAAU,EAAC;IACPD;EAEJ,CAAC;EACDE,KAAK,EAAC,CAAC,OAAO,EAAC,YAAY,EAAC,cAAc,CAAC;EAC3CC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAE,MAAM;MACbC,eAAe,EAAE,OAAO;MACxBC,SAAS,EAAC,cAAc;MACxBC,SAAS,EAAC;IACd;EACJ,CAAC;EAEDC,OAAO,EAAC;IACJC,WAAWA,CAAA,EAAE;MACT,IAAIL,KAAK,GAAC,OAAO;MACjB,IAAIC,eAAe,GAAC,OAAO;MAC3B,IAAIC,SAAS,GAAC,WAAW;MACzB,IAAIC,SAAS,GAAC,OAAM;MACpB,IAAG,IAAI,CAACH,KAAK,IAAE,OAAO,EAAE;QACpBA,KAAK,GAAC,MAAM;QACZC,eAAe,GAAC,OAAO;QACvBC,SAAS,GAAC,cAAc;QACxBC,SAAS,GAAC,OAAM;MACpB;MACA,IAAI,CAACH,KAAK,GAACA,KAAK;MAChB,IAAI,CAACC,eAAe,GAACA,eAAe;MACpC,IAAI,CAACC,SAAS,GAACA,SAAS;MACxB,IAAI,CAACC,SAAS,GAACA,SAAS;MACxB,IAAI,CAACG,iBAAiB,CAAC,CAAC;IAG5B,CAAC;IACDA,iBAAiBA,CAAA,EAAE;MACfC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAC,IAAI,CAACR,KAAK,CAA