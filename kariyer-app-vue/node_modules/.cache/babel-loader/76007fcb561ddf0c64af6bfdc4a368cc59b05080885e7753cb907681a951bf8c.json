adverts);\r\n      })\r\n      .catch(err => {\r\n        console.error('Error fetching adverts:', err);\r\n      });\r\n  },\r\n  mounted() {\r\n      this.ActiveUserId=localStorage.getItem(\"ActiveUserId\");\r\n      this.ActiveUserType=localStorage.getItem(\"ActiveUserType\");\r\n  },\r\n  methods: {\r\n    \r\n    sortTable(key) {\r\n      if (this.sortKey === key) {\r\n        this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\r\n      } else {\r\n        this.sortKey = key;\r\n        this.sortOrder = 'asc';\r\n      }\r\n    },\r\n    deleteAdvert(id) {\r\n      \r\n      axios.delete(`https://localhost:7190/advert/${id}`)\r\n      .catch(err => {\r\n            console.error('Error creating advert:', err);\r\n          });\r\n      this.$router.push(\"/adverts\");\r\n      location.reload();\r\n    },\r\n    editAdvert(advert) {\r\n      this.$router.push({ path: `/edit/${advert.id}`});\r\n    },\r\n    togglePublication(id) {\r\n      const advert = this.adverts.find(advert => advert.id === id);\r\n      if (advert) {\r\n        advert.isPublished = !advert.isPublished;\r\n        axios.put(`https://localhost:7190/advert/`, advert)\r\n          .then(() => {\r\n            // Başarıyla güncellendiğinde yapılacak işlemler\r\n          })\r\n          .catch(err => {\r\n            console.error('Error updating advert publication status:', err);\r\n          });\r\n      }\r\n    }\r\n  \r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":";AA+EA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AACjC,OAAOC,MAAK,MAAO,cAAc;AACjC,eAAe;EACbC,UAAU,EAAE;IACND;EACJ,CAAC;EACHE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,EAAE;MAClBC,UAAU,EAAE,EAAE;MACdC,gBAAgB,EAAC,IAAI;MACrBC,aAAa,EAAE,KAAK;MACpBC,eAAe,EAAE,KAAK;MACtBC,YAAY,EAAC,CAAC;MACdC,cAAc,EAAC;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACZ,OAAO,CAACa,MAAM,CAACC,MAAK,IAAK;QACjC,MAAMC,UAAS,GAAID