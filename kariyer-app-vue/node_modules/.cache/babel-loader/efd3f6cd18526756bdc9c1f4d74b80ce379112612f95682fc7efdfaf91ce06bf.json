{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  props: ['advertCreated'],\n  data() {\n    return {\n      advertTitle: '',\n      content: '',\n      salary: '',\n      experience: '',\n      published: true,\n      adverts: []\n    };\n  },\n  computed: {\n    isFormFilled() {\n      return !this.advertTitle || !this.content || !this.salary || !this.experience;\n    }\n  },\n  methods: {\n    submitForm() {\n      const advert = {\n        advertTitle: this.advertTitle,\n        content: this.content,\n        experience: this.experience,\n        salary: this.salary,\n        published: this.published\n      };\n\n      // localStorage güncelleme\n      let storedAdverts = JSON.parse(localStorage.getItem(\"adverts\")) || [];\n      storedAdverts.push(advert);\n      localStorage.setItem('adverts', JSON.stringify(storedAdverts));\n\n      // adverts.json dosyasını güncelleme\n      fetch('adverts.json').then(response => response.json()).then(adverts => {\n        adverts.push(advert);\n        fetch('adverts.json', {\n          method: 'PUT',\n          // PUT yerine POST kullanılması gerekebilir, backend API'sine bağlıdır.\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(adverts)\n        }).then(response => {\n          if (response.ok) {\n            console.log('adverts.json başarıyla güncellendi.');\n          } else {\n            console.error('adverts.json güncellenirken bir hata oluştu.');\n          }\n        });\n      }).catch(error => console.error('adverts.json okunurken bir hata oluştu:', error));\n    }\n  }\n};","map":{"version":3,"names":["props","data","advertTitle","content","salary","experience","published","adverts","computed","isFormFilled","methods","submitForm","advert","storedAdverts","JSON","parse","localStorage","getItem","push","setItem","stringify","fetch","then","response","json","method","headers","body","ok","console","log","error","catch"],"sources":["C:\\Users\\Furkan\\Documents\\Pratik kod\\NisoPratik\\kariyer-app\\src\\components\\createAdvert.vue"],"sourcesContent":["<template>\r\n    \r\n    <form action=\"\" class=\"container mb-3\" >\r\n        \r\n        <div class=\"row\">\r\n            <div class=\"col-md-8\">\r\n\r\n                <div class=\"mb-3\">\r\n                    <label for=\"\" class=\"form-label\">\r\n                        İlan Başlığı\r\n                    </label>\r\n                    <input \r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    v-model=\"advertTitle\"\r\n                    />\r\n\r\n                </div>\r\n\r\n                <div class=\"mb-3\">\r\n                    <label for=\"\" class=\"form-label\">\r\n                        Açıklama</label>\r\n                    <textarea \r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    row=\"5\"\r\n                    v-model=\"content\"></textarea>\r\n                </div>\r\n                \r\n            </div>\r\n            <div class=\"col\">\r\n\r\n                <div class=\"mb-3\">\r\n                    <label for=\"\" class=\"form-label\">\r\n                        Deneyim\r\n                    </label>\r\n                    <input type=\"text\" \r\n                    class=\"form-control\"\r\n                    v-model=\"experience\">\r\n                </div>\r\n\r\n                <div class=\"mb\">\r\n                    <label for=\"\" \r\n                    class=\"form-label\"\r\n                    >\r\n                        Maaş\r\n                    </label>\r\n                    <input type=\"text\" \r\n                    class=\"form-control\"\r\n                    v-model=\"salary\"/>\r\n                </div>\r\n\r\n                <div class=\"row mb-3\" >\r\n                    <div class=\"form-check\">\r\n                        <input type=\"checkbox\" class=\"form-check-input\" v-model=\"published\"/>\r\n                        <Label type=\"gridCheck1\" class=\"form-check-label\">\r\n                            Yayınla</Label>\r\n                    </div>  \r\n                </div>\r\n\r\n                <div class=\"mb-3 \" >\r\n                <button\r\n                :disabled=\"isFormFilled\"\r\n                class=\"btn btn-primary\"\r\n                @click.prevent=\"submitForm\">\r\n                İlanı Yarat</button>\r\n            </div>\r\n            </div>\r\n            \r\n        </div>\r\n    </form>\r\n    \r\n</template>\r\n<script>\r\nexport default{\r\n    props:['advertCreated'],\r\n    data(){\r\n        return{\r\n            \r\n            advertTitle:'',\r\n            content:'',\r\n            salary:'',\r\n            experience:'',\r\n            published:true,\r\n            adverts:[]\r\n        }\r\n    },\r\n    computed:{\r\n        isFormFilled(){\r\n            return !this.advertTitle || !this.content || !this.salary || !this.experience;\r\n        }\r\n    },\r\n    methods:{\r\n        submitForm() {\r\n        const advert = {\r\n            advertTitle: this.advertTitle,\r\n            content: this.content,\r\n            experience: this.experience,\r\n            salary: this.salary,\r\n            published: this.published\r\n        };\r\n        \r\n        // localStorage güncelleme\r\n        let storedAdverts = JSON.parse(localStorage.getItem(\"adverts\")) || [];\r\n        storedAdverts.push(advert);\r\n        localStorage.setItem('adverts', JSON.stringify(storedAdverts));\r\n\r\n        // adverts.json dosyasını güncelleme\r\n        fetch('adverts.json')\r\n            .then(response => response.json())\r\n            .then(adverts => {\r\n                adverts.push(advert);\r\n\r\n                fetch('adverts.json', {\r\n                    method: 'PUT', // PUT yerine POST kullanılması gerekebilir, backend API'sine bağlıdır.\r\