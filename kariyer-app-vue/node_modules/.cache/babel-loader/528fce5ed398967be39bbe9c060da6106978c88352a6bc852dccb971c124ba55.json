ate>\r\n  <div class=\"container mt-5\">\r\n    <table class=\"table table-striped\">\r\n      <thead>\r\n        <tr>\r\n          <th scope=\"col\" @click=\"sortTable('advertTitle')\">\r\n            İş Başlığı \r\n            <span v-if=\"sortKey === 'advertTitle'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('location')\">\r\n            Konum\r\n            <span v-if=\"sortKey === 'location'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('experience')\">\r\n            Deneyim\r\n            <span v-if=\"sortKey === 'experience'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\" @click=\"sortTable('isPublished')\">\r\n            Yayınlanma Durumu\r\n            <span v-if=\"sortKey === 'isPublished'\">{{ sortOrder === 'asc' ? '▲' : '▼' }}</span>\r\n          </th>\r\n          <th scope=\"col\">İşlemler</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(advert, index) in sortedAdverts\" :key=\"index\">\r\n          <td>{{ advert.jobTitle }}</td>\r\n          <td>{{ cityList(index) }}</td>\r\n          <td>{{ advert.experience }} Yıl</td>\r\n          <td>{{ advert.isPublished ? \"Yayınlandı\" : \"Yayınlanmadı\" }}</td>\r\n          <td>\r\n            <button class=\"btn btn-primary\" @click=\"editAdvert(index)\">Değiştir</button>\r\n            <button class=\"btn btn-danger\" @click=\"deleteAdvert(index)\">Sil</button>\r\n            <button type=\"button\" class=\"btn btn-warning\" @click=\"togglePublication(index)\">\r\n              {{ advert.isPublished ? \"Yayından kaldır\" : \"Yayınla\" }}\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      adverts: [],\r\n      sortKey: '',        \r\n      sortOrder: 'asc',\r\n      location:'',\r\n    };\r\n  },\r\n  computed: {\r\n    sortedAdverts() {\r\n      return this.adverts.slice().sort((a, b) => {\r\n        let modifier = this.sortOrder === 'asc' ? 1 : -1;\r\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\r\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\r\n        return 0;\r\n      });\r\n    },\r\n    cityList(index) {\r\n    return (index) => {\r\n      // Şehirleri liste olarak birleştirip virgülle ayırarak döndür\r\n      return this.adverts[index].location.join(', ');\r\n    };\r\n  }\r\n    \r\n  },\r\n  created() {\r\n    this.adverts = JSON.parse(localStorage.getItem('adverts')) || [];\r\n  },\r\n  methods:{\r\n    sortTable(key) {\r\n      if (this.sortKey === key) {\r\n        this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\r\n      } else {\r\n        this.sortKey = key;\r\n        this.sortOrder = 'asc';\r\n      }\r\n    },\r\n    deleteAdvert(index) {\r\n      this.adverts.splice(index, 1);\r\n      localStorage.setItem('adverts', JSON.stringify(this.adverts));\r\n    },\r\n    editAdvert(index) {\r\n      this.$router.push({ path: `/edit/${index}` });\r\n    },\r\n    togglePublication(index) {\r\n      this.adverts[index].isPublished = !this.adverts[index].isPublished;\r\n      localStorage.setItem('adverts', JSON.stringify(this.adverts));\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";AA4CA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,KAAK;MAChBC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACL,OAAO,CAACM,KAAK,CAAC,CAAC,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACzC,IAAIC,QAAO,GAAI,IAAI,CAACR,SAAQ,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QAChD,IAAIM,CAAC,CAAC,IAAI,CAACP,OAAO,IAAIQ,CAAC,CAAC,IAAI,CAACR,OAAO,CAAC,EAAE,OAAO,CAAC,IAAIS,QAAQ;QAC3D,IAAIF,CAAC,CAAC,IAAI,CAACP,OAAO,IAAIQ,CAAC,CAAC,IAAI,CAACR,OAAO,CAAC,EAAE,OAAO,IAAIS,QAAQ;QAC1D,OAAO,CAAC;MACV,CA