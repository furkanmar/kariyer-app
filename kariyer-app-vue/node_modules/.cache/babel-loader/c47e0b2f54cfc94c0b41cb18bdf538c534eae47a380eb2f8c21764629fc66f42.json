ts.flatMap(advert => advert.locations);\r\n      return [...new Set(locations)];\r\n    },\r\n    filteredAdverts() {\r\n      return this.adverts\r\n        .filter(advert => {\r\n          const search = this.searchQuery.toLowerCase();\r\n          return (\r\n            advert.jobTitle.toLowerCase().includes(search) ||\r\n            advert.locations.join(\",\").toLowerCase().includes(search) ||\r\n            advert.experience.toString().includes(search)\r\n          );\r\n        })\r\n        .filter(advert => {\r\n          if (!this.locationFilter) return true;\r\n          return advert.locations.includes(this.locationFilter);\r\n        });\r\n    },\r\n    sortedAdverts() {\r\n      return this.filteredAdverts.slice().sort((a, b) => {\r\n        let modifier = this.sortOrder === 'asc' ? 1 : -1;\r\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\r\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\r\n        return 0;\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    this.adverts = JSON.parse(localStorage.getItem('adverts')) || [];\r\n  },\r\n  methods: {\r\n    sortTable(key) {\r\n      if (this.sortKey === key) {\r\n        this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\r\n      } else {\r\n        this.sortKey = key;\r\n        this.sortOrder = 'asc';\r\n      }\r\n    },\r\n    deleteAdvert(index) {\r\n      this.adverts.splice(index, 1);\r\n      localStorage.setItem('adverts', JSON.stringify(this.adverts));\r\n    },\r\n    editAdvert(index) {\r\n      this.$router.push({ path: `/edit/${index}` });\r\n    },\r\n    togglePublication(index) {\r\n      this.adverts[index].isPublished = !this.adverts[index].isPublished;\r\n      localStorage.setItem('adverts', JSON.stringify(this.adverts));\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";;;;;;;;AA6DA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,EAAC